<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.untitledPrototype.controller.Page2" xmlns:tnt="sap.tnt"
	xmlns="sap.m" xmlns:layout="sap.ui.layout">
	<tnt:ToolPage sideExpanded="true">
		<tnt:header>
			<tnt:ToolHeader height="50px" width="auto" design="Transparent" visible="false" enabled="true">
				<tnt:content>
					<Button text="" type="Default" icon="sap-icon://menu2" iconFirst="true" width="auto" enabled="true" visible="true"
						tooltip="Toggle Side Navigation" press="_onToggleButtonPress"/>
					<Button text="Home" type="Transparent" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
						press="_onButtonPress"/>
					<ToolbarSpacer width="20px"/>
					<Button text="" type="Transparent" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
					<ToolbarSpacer width=""/>
					<ToolbarSpacer width=""/>
					<Button text="Conference" type="Transparent" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
				</tnt:content>
			</tnt:ToolHeader>
		</tnt:header>
		<tnt:sideContent>
			<tnt:SideNavigation visible="true">
				<tnt:item>
					<tnt:NavigationList width="auto" expanded="true">
						<tnt:items>
							<tnt:NavigationListItem icon="sap-icon://e-learning" expanded="true" hasExpander="true" text="OOP Programming" select="_onNavigationListItemSelect">
								<tnt:items>
									<tnt:NavigationListItem icon="" expanded="true" hasExpander="true" text="Events" select="_onNavigationListItemSelect1">
										<tnt:items/>
									</tnt:NavigationListItem>
									<tnt:NavigationListItem icon="" expanded="true" hasExpander="true" text="Resources" select="_onNavigationListItemSelect2">
										<tnt:items/>
									</tnt:NavigationListItem>
									<tnt:NavigationListItem icon="" expanded="true" hasExpander="true" text="Contacts" select="_onNavigationListItemSelect3">
										<tnt:items/>
									</tnt:NavigationListItem>
									<tnt:NavigationListItem expanded="true" hasExpander="true" text="Documentation" select="_onNavigationListItemFirstLevelSelect"/>
									<tnt:NavigationListItem expanded="true" hasExpander="true" text="Forum" select="_onNavigationListItemFirstLevelSelect1"/>
								</tnt:items>
							</tnt:NavigationListItem>
							<tnt:NavigationListItem icon="sap-icon://e-learning" expanded="true" hasExpander="true" text="Procedural Programming">
								<tnt:items>
									<tnt:NavigationListItem expanded="true" hasExpander="true" text="Events" select="_onNavigationListItemSelect4"/>
									<tnt:NavigationListItem expanded="true" hasExpander="true" text="Resources" select="_onNavigationListItemSelect5"/>
									<tnt:NavigationListItem expanded="true" hasExpander="true" text="Contacts" select="_onNavigationListItemSelect3"/>
									<tnt:NavigationListItem expanded="true" hasExpander="true" text="Documentation" select="_onNavigationListItemSelect6"/>
									<tnt:NavigationListItem expanded="true" hasExpander="true" text="Forum" select="_onNavigationListItemFirstLevelSelect1"/>
								</tnt:items>
							</tnt:NavigationListItem>
						</tnt:items>
					</tnt:NavigationList>
				</tnt:item>
				<tnt:fixedItem>
					<tnt:NavigationList width="auto" expanded="true">
						<tnt:items>
							<tnt:NavigationListItem icon="sap-icon://employee" expanded="true" hasExpander="true" text="Item 1">
								<tnt:items/>
							</tnt:NavigationListItem>
							<tnt:NavigationListItem icon="sap-icon://building" expanded="true" hasExpander="true" text="Item 2">
								<tnt:items/>
							</tnt:NavigationListItem>
							<tnt:NavigationListItem icon="sap-icon://card" expanded="true" hasExpander="true" text="Item 3">
								<tnt:items/>
							</tnt:NavigationListItem>
						</tnt:items>
					</tnt:NavigationList>
				</tnt:fixedItem>
				<tnt:footer/>
			</tnt:SideNavigation>
		</tnt:sideContent>
		<tnt:mainContents>
			<layout:BlockLayout background="Default" visible="true">
				<layout:content>
					<layout:BlockLayoutRow scrollable="false">
						<layout:content>
							<layout:BlockLayoutCell width="1" title="OOP Programming" titleAlignment="Begin" titleLevel="Auto">
							</layout:BlockLayoutCell>
						</layout:content>
					</layout:BlockLayoutRow>
				</layout:content>
			</layout:BlockLayout>
			<Carousel height="400px" width="100%" loop="true" showPageIndicator="true" pageIndicatorPlacement="Bottom" visible="true"
				class="sapUiContentPadding">
				<pages>
					<Image src="/webapp/resources/55877.png" height="500px" width="500px" visible="true" densityAware="false"/>
					<Image src="/webapp/resources/descarga.jpg" height="800px" width="500px" visible="true" densityAware="false"/>
					<Image src="/webapp/resources/55877.png" height="500px" width="500px" visible="true" densityAware="false"/>
					<Image src="/build/uilibraries/content/static/image-canvas.svg" height="auto" width="50px" visible="true" densityAware="false"/>
					<Image src="/build/uilibraries/content/static/image-canvas.svg" height="auto" width="50px" visible="true" densityAware="false"/>
					<Image src="/webapp/resources/descarga.png" height="154px" width="327px" visible="true" densityAware="false"/>
				</pages>
			</Carousel>
			<VBox alignItems="Center" direction="Column" fitContainer="true" width="auto" height="auto" justifyContent="End" renderType="Div"
				visible="true" displayInline="false" class="sapUiTinyMargin">
				<items>
					<Title text="Conference" titleStyle="H4" width="auto" textAlign="Begin" visible="true" wrapping="false"/>
					<Text text="OOP Programming conference" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
					<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
						visible="true" displayInline="false">
						<items>
							<Text text="Link to the conference:" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
							<Link href="https://www.youtube.com/watch?v=EstUXL9V-0k" text="Join conference" textAlign="Begin" width="auto" target="_blank"
								wrapping="false" enabled="true" visible="true" emphasized="false" subtle="false"/>
						</items>
						<layoutData/>
					</HBox>
				</items>
				<layoutData/>
			</VBox>
			<VBox alignItems="Center" direction="Column" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
				visible="true" displayInline="false" class="sapUiTinyMargin">
				<items>
					<Title text="Code Review" titleStyle="H4" width="auto" textAlign="Center" visible="true" wrapping="false"/>
					<Text text="Participants:" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
					<FormattedText htmlText="&lt;li>Noah Miller&lt;/li>&lt;li>Liam Davis&lt;/li>" width="auto" visible="true" class="sapUiTinyMargin"/>
					<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
						visible="true" displayInline="false">
						<items>
							<Text text="Link to code:" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
							<Link href="https://www.universal-robots.com/articles/ur-articles/repeating-a-program-section-loop/" text="Code Review" textAlign="Begin"
								width="auto" target="_blank" wrapping="false" enabled="true" visible="true" emphasized="false" subtle="false"/>
						</items>
						<layoutData/>
					</HBox>
				</items>
				<layoutData/>
			</VBox>
			<VBox alignItems="Center" direction="Column" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
				visible="true" displayInline="false">
				<items>
					<Title text="Pair Programming" titleStyle="H4" width="auto" textAlign="Center" visible="true" wrapping="false"/>
					<HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div"
						visible="true" displayInline="false">
						<items>
							<Text text="Link to pair progrmming:" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
							<Link href="https://github.com/ros-industrial/universal_robot/blob/kinetic-devel/ur_description/urdf/ur3_joint_limited_robot.urdf.xacro"
								text="Pair programming" textAlign="Begin" width="auto" target="_blank" wrapping="false" enabled="true" visible="true" emphasized="false"
								subtle="false"/>
						</items>
						<layoutData/>
					</HBox>
				</items>
				<layoutData/>
			</VBox>
		</tnt:mainContents>
	</tnt:ToolPage>
</mvc:View>